  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "quantity": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "orderItems": {
    "type": "hasMany",
    "model": "OrderItem",
    "foreignKey": "productId"
  },
  "orders": {
    "type": "hasMany",
    "model": "orders",
    "through": "OrderItem",
    "foreignKey": "productId",
    "keyThrough": "orderId"
  }
  },
  "acls": [],
  "methods": {}
}
