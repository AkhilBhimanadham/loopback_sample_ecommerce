{
  "name": "AppUser",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "password": {
      "type": "string",
      "required": true
    },
    "role":{
      "type": "number",
      "required": false,
      "defaultFn": 0
    },
    "created_at": {
      "type": "date",
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "orders": {
      "type": "hasMany",
      "model": "orders",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
